    "@navikt/nav-dekoratoren-moduler": "^2.1.6",
    "@navikt/oasis": "^3.3.0",
    "@portabletext/react": "^3.1.0",
    "@portabletext/types": "^2.0.13",
    "@remix-run/css-bundle": "^2.10.0",
    "@remix-run/express": "^2.10.0",
