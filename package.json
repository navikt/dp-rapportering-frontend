    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.4"
  },
  "overrides": {
    "@remix-run/server-runtime": {
      "cookie": "0.7.2"
    },
    "remix-validated-form": {
      "nanoid": "3.3.8"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c tsc",
      "bash -c 'npm run lint'"
    ]
  }
}
