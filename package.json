{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && run-p build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev",
    "start": "ts-node ./server/server.ts",
    "typecheck": "tsc --project tsconfig.json",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --fix --ext .ts,.tsx",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "pre-commit": "lint-staged && npm run typecheck"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^5.5.0",
    "@navikt/dp-auth": "^0.3.7",
    "@navikt/ds-css": "^5.5.0",
    "@navikt/ds-react": "^5.5.0",
    "@navikt/nav-dekoratoren-moduler": "^2.1.3",
    "@portabletext/types": "^2.0.6",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/express": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@remix-validated-form/with-zod": "^2.0.6",
    "@sanity/client": "^6.4.12",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "groq": "^3.16.7",
    "html-react-parser": "^4.2.2",
    "husky": "^8.0.3",
    "isbot": "^3.7.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^5.1.5",
    "tiny-invariant": "^1.3.1",
    "tinyduration": "^3.3.0",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@remix-run/testing": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.6",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-istanbul": "^0.34.5",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "npm run lint",
      "bash -c 'npm run typecheck'"
    ]
  }
}
