{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && run-p build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev",
    "start": "ts-node ./server/server.ts",
    "typecheck": "tsc --project tsconfig.json",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --fix --ext .ts,.tsx",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "pre-commit": "lint-staged && npm run typecheck"
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.7.2",
    "@navikt/aksel-icons": "^6.8.0",
    "@navikt/ds-css": "^6.8.0",
    "@navikt/ds-react": "^6.8.0",
    "@navikt/nav-dekoratoren-moduler": "^2.1.6",
    "@navikt/oasis": "^3.3.0",
    "@portabletext/types": "^2.0.13",
    "@remix-run/css-bundle": "^2.9.2",
    "@remix-run/express": "^2.9.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@sanity/client": "^6.18.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "groq": "^3.42.0",
    "html-react-parser": "^5.1.10",
    "husky": "^8.0.3",
    "isbot": "^5.1.6",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-validated-form": "^5.1.5",
    "tiny-invariant": "^1.3.3",
    "tinyduration": "^3.3.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@remix-run/testing": "^2.9.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "npm run lint",
      "bash -c 'npm run typecheck'"
    ]
  }
}
