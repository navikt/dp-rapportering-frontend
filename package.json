{
  "name": "dp-rapportering",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "prepare": "husky",
    "lint": "eslint",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "pre-commit": "lint-staged && npm run typecheck"
  },
  "dependencies": {
    "@faker-js/faker": "9.8.0",
    "@grafana/faro-web-sdk": "1.18.2",
    "@mswjs/data": "0.16.2",
    "@navikt/aksel-icons": "7.22.0",
    "@navikt/ds-css": "7.22.0",
    "@navikt/ds-react": "7.22.0",
    "@navikt/nav-dekoratoren-moduler": "3.2.3",
    "@navikt/oasis": "3.8.0",
    "@portabletext/react": "3.2.1",
    "@portabletext/types": "2.0.13",
    "@remix-run/css-bundle": "2.16.8",
    "@remix-run/express": "2.16.8",
    "@remix-run/node": "2.16.8",
    "@remix-run/react": "2.16.8",
    "@remix-run/serve": "2.16.8",
    "@remix-validated-form/with-zod": "2.0.7",
    "@sanity/client": "7.6.0",
    "classnames": "2.5.1",
    "compression": "1.8.0",
    "cross-env": "7.0.3",
    "date-fns": "4.1.0",
    "express": "4.21.2",
    "groq": "3.92.0",
    "html-react-parser": "5.2.5",
    "isbot": "5.1.28",
    "morgan": "1.10.0",
    "msw": "2.10.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remeda": "2.23.0",
    "remix-validated-form": "5.1.5",
    "tiny-invariant": "1.3.3",
    "tinyduration": "3.4.1",
    "uuidv7": "1.0.2",
    "winston": "3.17.0",
    "zod": "3.25.64",
    "zod-form-data": "2.0.7"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@remix-run/dev": "2.16.8",
    "@remix-run/testing": "2.16.8",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/compression": "1.8.1",
    "@types/express": "5.0.3",
    "@types/morgan": "1.9.10",
    "@types/node": "22.15.31",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-istanbul": "3.1.4",
    "dotenv": "16.5.0",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.0",
