{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && run-p build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev",
    "start": "ts-node ./server/server.ts",
    "typecheck": "tsc",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --fix --ext .ts,.tsx",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "format": "prettier --write",
    "pre-commit": "lint-staged && npm run typecheck"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^4.0.0",
    "@navikt/dp-auth": "^0.3.3",
    "@navikt/ds-css": "^2.1.7",
    "@navikt/ds-react": "^2.1.7",
    "@navikt/nav-dekoratoren-moduler": "^2.0.1",
    "@portabletext/types": "^2.0.2",
    "@remix-run/css-bundle": "^1.16.1",
    "@remix-run/express": "^1.16.1",
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/serve": "^1.12.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@sanity/client": "^6.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "groq": "^3.11.1",
    "html-react-parser": "^3.0.16",
    "husky": "^8.0.3",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^4.6.11",
    "tiny-invariant": "^1.3.1",
    "tinyduration": "^3.2.4",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@remix-run/testing": "^1.16.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
