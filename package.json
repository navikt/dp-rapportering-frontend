{
  "name": "dp-rapportering",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "prepare": "husky",
    "lint": "eslint",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "pre-commit": "lint-staged && npm run typecheck"
  },
  "dependencies": {
    "@faker-js/faker": "9.5.0",
    "@grafana/faro-web-sdk": "1.13.3",
    "@mswjs/data": "0.16.2",
    "@navikt/aksel-icons": "7.14.1",
    "@navikt/ds-css": "7.14.1",
    "@navikt/ds-react": "7.14.1",
    "@navikt/nav-dekoratoren-moduler": "3.2.2",
    "@navikt/oasis": "3.7.0",
    "@portabletext/react": "3.2.1",
    "@portabletext/types": "2.0.13",
    "@remix-run/css-bundle": "2.15.3",
    "@remix-run/express": "2.15.3",
    "@remix-run/node": "2.15.3",
    "@remix-run/react": "2.15.3",
    "@remix-run/serve": "2.15.3",
    "@remix-validated-form/with-zod": "2.0.7",
    "@sanity/client": "6.28.0",
    "classnames": "2.5.1",
    "compression": "1.8.0",
    "cross-env": "7.0.3",
    "date-fns": "4.1.0",
    "express": "4.21.2",
    "groq": "3.76.3",
    "html-react-parser": "5.2.2",
    "isbot": "5.1.23",
    "morgan": "1.10.0",
    "msw": "2.7.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remeda": "2.20.2",
    "remix-validated-form": "5.1.5",
    "tiny-invariant": "1.3.3",
    "tinyduration": "3.4.1",
    "uuidv7": "1.0.2",
    "winston": "3.17.0",
    "zod": "3.24.2",
    "zod-form-data": "2.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.20.0",
    "@remix-run/dev": "2.15.3",
    "@remix-run/testing": "2.15.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.0",
    "@types/morgan": "1.9.9",
    "@types/node": "22.13.11",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-istanbul": "2.1.8",
    "dotenv": "16.4.7",
    "eslint": "9.20.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "vite": "5.4.14",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "overrides": {
    "@remix-run/server-runtime": {
      "cookie": "0.7.2"
    },
    "remix-validated-form": {
      "nanoid": "3.3.8"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c tsc",
      "bash -c 'npm run lint'"
    ]
  }
}
