    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c tsc",
      "bash -c 'npm run lint'"
    ]
  }
}
